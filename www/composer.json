{
    "name": "broadway/broadway-demo",
    "description": "broadway",
    "license": "MIT",
    "authors": [
        {
            "name": "Fritsjan",
            "email": "fritsjan@qandidate.com"
        },
        {
            "name": "Willem-Jan",
            "email": "wjzijderveld@gmail.com"
        },
        {
            "name": "Qandidate.com",
            "homepage": "http://labs.qandidate.com"
        }
    ],
    "require": {
        "broadway/broadway": "^1.0",
        "broadway/broadway-bundle": "^0.2",
        "broadway/event-store-dbal": "^0.1",
        "broadway/read-model-elasticsearch": "^0.1",
        "symfony/browser-kit": "~2.8",
        "symfony/console": "~2.8",
        "symfony/class-loader": "~2.8",
        "symfony/framework-bundle": "~2.8",
        "symfony/yaml": "~2.8",
        "symfony/finder": "~2.8",
        "qandidate/stack-request-id": "~0.3",
        "symfony/process": "~2.6",
        "sensio/distribution-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "instaclick/base-test-bundle": "~0.5",
        "phpunit/phpunit": "^5.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    }
}
